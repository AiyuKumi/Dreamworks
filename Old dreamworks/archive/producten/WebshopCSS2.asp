  margin-top: 5px;
  padding-right:20px;
  text-align:right;
/*  text-decoration:none;*/
}
#shopcontainer #winkelwagen .afrekenen {
  background:transparent url('images/winkel-knop-afrekenen.png') no-repeat right center;
  color:#fff;
  display:block;
  font-size:11px;
  line-height:20px;
  padding-right:20px;
  text-align:right;
/*  text-decoration:none;*/
}

