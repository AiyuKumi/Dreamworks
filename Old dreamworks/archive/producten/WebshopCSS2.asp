


#shopcontainer {
  cursor: default;
  font-family: Arial;
  font-size: 11px;
  width:900px;
  line-height: 1.6em;
  letter-spacing: 0px;
  text-align: left;
}
#shopcontainer * {
  margin: 0;
  outline: 0;
  padding: 0;
}
#shopcontainer a {
  font-weight: normal;
  text-decoration: underline;
}
#shopcontainer a:hover {
  text-decoration: none;
}
#shopcontainer .product_inhoud2 fieldset {
  border: 0;
}
#shopcontainer .product_inhoud2 .ref_nr {
  float: right;
}
#shopcontainer h2 {
  color: black;
  font-size: 18px;
  margin-bottom:3px;
  line-height: 24px;
  text-align:left;
}
#shopcontainer img {
  border:0;
}

/* KRUIMELPAD */
#shopcontainer #kruimelpad {
  font-size: 10px;
  margin-bottom: 5px;
}
#shopcontainer #kruimelpad a {
  color: white;
}

/* MENU WEBSHOP */
#shopcontainer #WebshopMenu {
  clear: left;
  float: left;
  margin-bottom:1em;
  width: 150px;
}
#shopcontainer #WebshopMenu ul {
  list-style: none;
  margin-left:5px;
  margin-top:3px;
}
#shopcontainer #WebshopMenu ul li.first {
  margin-left: 0px;
}
#shopcontainer #WebshopMenu ul li  {
  margin-left: 10px;
}
#shopcontainer #WebshopMenu ul li li {
  margin-left: 0px;
  padding-left:10px;
}
#shopcontainer #WebshopMenu ul tt {
  position: absolute;
  margin-left: -10px;
}
#shopcontainer #WebshopMenu ul ul {
  border-left:1px dotted silver;
  margin-left:-7px;
  padding-left:3px;
}
#shopcontainer #WebshopMenu ul a {
  color: #ffffff;
  text-decoration: none;
}
#shopcontainer #WebshopMenu ul a:hover {
  color: #ffffff;
  text-decoration: underline;
}

/* ZOEKFUNCTIE */
#shopcontainer #zoekfunctie {
  clear: left;
  float: left;
  width: 150px;
  margin-bottom:1em;
}
#shopcontainer #zoekfunctie .zoeken_input {
  border:1px solid gray;
  font-size: 12px;
  font-family: arial;
  height: 18px;
  width: 125px;
}
#shopcontainer #zoekfunctie .zoeken_button {
  border: 0;
  cursor:pointer;
  border-right:1px solid silver;
  position: absolute;
  *margin-left: -5px;
  *margin-top: 1px;
}

.product_nav_form{
color:#000;
}

#winkelbody{
color:#fff;
}

.prijslijst {
color:#fff;
}

.prijslijst_totaal {
color:#fff;
}

/* WINKELWAGEN */
#shopcontainer #winkelwagen {
  clear: left;
  float: left;
  width: 150px;
}
#shopcontainer #winkelwagen .prijslijst {
  border-bottom: 1px dotted silver;
  font-size:10px;
}
